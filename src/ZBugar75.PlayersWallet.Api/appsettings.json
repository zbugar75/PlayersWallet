{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "ZBugar75.PlayersWallet": "Information"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "Application": "ZBugar75.PlayersWallet"
    }
  },
  "AllowedHosts": "*",
  "SimpleMemoryCache": {
    "SizeLimit": 1024, // Maximal entries in memory cache
    "SlidingExpiration": "1.00:00:00", // Keep in cache for this time, reset time if accessed. It hast to be > 0. Format hh:mm:ss or d.hh:mm:ss
    "AbsoluteExpiration": "1.00:00:00" // Keep in cache for this time, reset time if accessed. It hast to be > 0. Format hh:mm:ss or d.hh:mm:ss
  }
}